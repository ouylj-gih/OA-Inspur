{
  "name": "RoleMap",
  "description": "角色映射表",
  "base": "RoleMapping",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {},
  "validations": [],
  "relations": {
    "employeeRole": {
      "type": "belongsTo",
      "model": "EmployeeRole",
      "foreignKey": "roleId"
    },
    "employee": {
      "type": "belongsTo",
      "model": "Employee",
      "foreignKey": "principalId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "superAdmin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
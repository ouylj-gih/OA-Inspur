{
    "name": "LogRecord",
    "description": "日志记录",
    "base": "PersistedModel",
    "idInjection": true,
    "indexes": {},
    "options": {
        "validateUpsert": true
    },
    "properties": {
        "modifier": {
            "type": "number",
            "required": true,
            "description": "修改人ID"
        },
        "display": {
            "type": "number",
            "required": true,
            "description": "是否已删除: -1-已删除, 1-正常",
            "default": 1
        },
        "detail": {
            "type": "string",
            "required": true,
            "description": "日志详情"
        },
        "create_time": {
            "type": "date",
            "required": true,
            "description": "创建时间",
            "defaultFn": "now"
        }
    },
    "validations": [],
    "relations": {},
    "acls": [
        {
            "accessType": "*",
            "principalType": "ROLE",
            "principalId": "$everyone",
            "permission": "DENY"
        },
        {
            "accessType": "*",
            "principalType": "ROLE",
            "principalId": "superAdmin",
            "permission": "ALLOW"
        }
    ],
    "methods": {}
}